<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="kr.ac.jbnu.se.mobile.ganogano.NoteFirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/note_fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>

    <fragment
        android:id="@+id/SecondFragment"
        android:name="kr.ac.jbnu.se.mobile.ganogano.NoteSecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/note_fragment_second">

        <argument
            android:name="hospital_name"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="default" />
        <action
            android:id="@+id/action_SecondFragment_to_ThirdFragment"
            app:destination="@id/ThirdFragment" />
    </fragment>

    <fragment
        android:id="@+id/ThirdFragment"
        android:name="kr.ac.jbnu.se.mobile.ganogano.NoteThirdFragment"
        android:label="note_fragment_third"
        tools:layout="@layout/note_fragment_third" >
        <action
            android:id="@+id/action_ThirdFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
</navigation>
